{
  "_version": "1.76.0",
  "sap.app": {
    "id": "eshm",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.20.0",
      "toolsId": "f1ce95f1-442f-4ab6-91ee-ca10f3a9e303"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZESHM_PR_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.143.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.tnt": {},
        "sap.suite.ui.microchart": {},
        "sap.ui.layout": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "eshm.i18n.i18n",
          "supportedLocales": [
            "",
            "en"
          ],
          "fallbackLocale": "en"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "eshm.view",
        "async": true,
        "viewPath": "eshm.view"
      },
      "routes": [
        {
          "name": "RouteLogin",
          "pattern": "",
          "target": [
            "TargetLogin"
          ]
        },
        {
          "name": "RouteDashboard",
          "pattern": "dashboard",
          "target": [
            "TargetDashboard"
          ]
        }
      ],
      "targets": {
        "TargetLogin": {
          "id": "Login",
          "name": "Login"
        },
        "TargetDashboard": {
          "id": "Dashboard",
          "name": "Dashboard"
        }
      }
    },
    "rootView": {
      "viewName": "eshm.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}